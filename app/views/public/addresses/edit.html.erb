<h2>配送先編集</h2>

<%=form_with model: @address,url: address_path,method: :path do |f| %>

 <p></p><tr><th>郵便番号(ハイフンなし)</th><td><%=f.text_field:postal_code%></td></tr></p>
 <p><tr><th>住所</th><td><%=f.text_field:address%></td></tr></p>
 <p><tr><th>宛名</th><td><%=f.text_field:name%></td><td><%=f. submit '変更内容を保存'%></td></tr>